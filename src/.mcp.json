{
    "mcpServers": {
        "context7": {
            "type": "http",
            "url": "https://mcp.context7.com/mcp",
            "headers": {
                "CONTEXT7_API_KEY": "{API_KEY}"
            }
        },
        "vscode-mcp": {
            "command": "cmd",
            "args": [
                "/c",
                "npx",
                "-y",
                "@vscode-mcp/vscode-mcp-server@latest"
            ],
            "env": {
                "VSCODE_MCP_ENABLED_TOOLS": "get_symbol_lsp_info,get_diagnostics,get_references,health_check"
            }
        }
    }
}