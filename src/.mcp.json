{
    "mcpServers": {
        "context7": {
            "type": "http",
            "url": "https://mcp.context7.com/mcp",
            "headers": {
                "CONTEXT7_API_KEY": "{API_KEY}"
            }
        },
        "sequential-thinking": {
            "command": "npx",
            "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
        },
        "db-mcp-server": {
            "command": "/Users/conte/go/bin/server",
            "args": ["-t", "stdio", "-c", "/Users/conte/db-mcp-config.json", "-lazy-loading", "-unified-tools"]
        }
    }
}
