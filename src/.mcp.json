{
    "mcpServers": {
        "context7": {
            "type": "http",
            "url": "https://mcp.context7.com/mcp",
            "headers": {
                "CONTEXT7_API_KEY": "{API_KEY}"
            }
        },
        "sequential-thinking": {
            "command": "cmd",
            "args": [
                "/c",
                "npx",
                "-y",
                "@modelcontextprotocol/server-sequential-thinking"
            ]
        },
        "dbhub_pr": {
            "command": "cmd",
            "args": [
                "/c",
                "npx",
                "-y",
                "@bytebase/dbhub",
                "--config",
                "dbhub_pr.toml"
            ]
        },
        "dbhub_cp": {
            "command": "cmd",
            "args": [
                "/c",
                "npx",
                "-y",
                "@bytebase/dbhub",
                "--config",
                "dbhub_cp.toml"
            ]
        }
    }
}